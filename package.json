{
  "name": "xcode",
  "displayName": "xcode",
  "publisher": "AmanVishnoi777",
  "description": "Visual Studio Code extension for AI-powered code completion.",
  "repository": {
    "url": ""
  },
  "keywords": [
    "AI",
    "gpt",
    "gpt3",
    "code completion",
    "code suggestions",
    "Github Copilot",
    "VS Code",
    "Visual Studio Code",
    "extension"
  ],
  "version": "0.1.3",
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "xcode.suggest",
        "title": "xcode: Suggest code"
      }
    ],
    "configuration": {
      "title": "GPilot",
      "properties": {
        "gpilot.apiKey": {
          "type": "string",
          "default": false,
          "description": "Open AI API key (https://platform.openai.com/account/api-keys)"
        }
      }
    },
    "keybindings": [
      {
        "command": "xcode.suggest",
        "key": "Ctrl+Alt+space"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/node-fetch": "^2.6.2",
    "@types/vscode": "^1.76.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@vscode/test-electron": "^2.2.3",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.34.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "string-replace-loader": "^3.1.0",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "chalk": "^5.2.0",
    "chatgpt": "^5.0.10",
    "node-fetch": "^2.6.9",
    "winston": "^3.8.2"
  }
}
